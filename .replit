modules = ["nodejs-20"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Test Server"

[[workflows.workflow]]
name = "Test Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node test-server.js"
waitForPort = 5000

[deployment]
run = ["sh", "-c", "node test-server.js"]

[[ports]]
localPort = 5000
externalPort = 80
